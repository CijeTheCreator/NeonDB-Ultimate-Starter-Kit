{
  "name": "nextjs-auth0",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev_": "next dev",
    "watch:server": "nodemon ./server/app.ts",
    "hasura": "docker compose up --build",
    "dev": "concurrently --kill-others \"npm run dev_\" \"npm run watch:server\" \"npm run hasura\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "prepare": "husky install",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "init-project": "node quickstart"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^2.6.2",
    "@types/express": "^4.17.21",
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/node": "20.3.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "autoprefixer": "10.4.14",
    "dotenv": "^16.4.5",
    "eslint-config-next": "13.4.7",
    "express": "^4.19.2",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "libsodium-wrappers": "^0.7.15",
    "morgan": "^1.10.0",
    "next": "13.4.7",
    "octokit": "^4.0.2",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "readline-sync": "^1.4.10",
    "simple-git": "^3.25.0",
    "tailwindcss": "3.3.2",
    "tweetsodium": "^0.0.6",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/supertest": "^6.0.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.9.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prettier": "3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.0",
    "typescript-eslint": "^8.3.0"
  }
}
